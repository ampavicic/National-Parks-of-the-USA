            "examples": [
              "1919-01-26T00:00:00.000Z"
            ]
          },
          "area": {
            "$id": "#/items/anyOf/0/properties/area",
            "type": "number",
            "title": "Area",
            "description": "Area of the National Park in square km",
            "default": 0.0,
            "examples": [
              198.6
            ]
          },
          "visitors": {
            "$id": "#/items/anyOf/0/properties/visitors",
